{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm dev",
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "docker dev",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm docker:dev",
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "lint fix",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm lint:fix",
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "format fix",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm format:fix",
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**"
      ]
