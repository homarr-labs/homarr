{
  "extends": "@homarr/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
  },
  "include": [".", ".next/types/**/*.ts", "src/app/[locale]/boards/(content)/_widget-option-settings-context.tsx"],
  "exclude": ["node_modules", ".next"]
}
