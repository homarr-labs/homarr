{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^@homarr/"
      ],
      "enabled": false
    },
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ],
  "updateInternalDeps": true,
  "rangeStrategy": "bump",
  "automerge": false,
  "baseBranches": [
    "dev"
  ],
  "dependencyDashboard": false
}